---
import BaseLayout from '../../layouts/BaseLayout.astro';
---

<BaseLayout title="Masterminds">
  <!-- Hero Section -->
  <section class="bg-gradient-to-br from-monica-bg to-white py-12 lg:py-16">
    <div class="container mx-auto px-4 sm:px-6 lg:px-8">
      <div class="max-w-6xl mx-auto">
        <div class="text-center mb-12">
          <h1 class="text-4xl lg:text-5xl font-serif font-bold text-monica-navy mb-4" data-cms="mastermind-hero-title">
            Entrepreneur & Leadership Masterminds
          </h1>
          <p class="text-xl text-monica-teal font-medium mb-2" data-cms="mastermind-hero-subtitle">
            Accelerate Your Success Through the Power of Community
          </p>
          <p class="text-lg text-monica-dark max-w-3xl mx-auto" data-cms="mastermind-hero-description">
            Join a curated group of ambitious professionals and entrepreneurs committed to breakthrough growth. Together, we'll challenge limitations, share wisdom, and achieve extraordinary results.
          </p>
        </div>

        <!-- CTA Buttons -->
        <div class="flex flex-col sm:flex-row gap-4 justify-center">
          <a href="/contact#consultation" class="btn-primary inline-flex items-center justify-center">
            <span>Apply for Next Cohort</span>
            <svg class="w-5 h-5 ml-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path>
            </svg>
          </a>
          <a href="#how-it-works" class="btn-secondary inline-flex items-center justify-center">
            <span>Learn More</span>
            <svg class="w-5 h-5 ml-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"></path>
            </svg>
          </a>
        </div>
      </div>
    </div>
  </section>

  <!-- What is a Mastermind Section -->
  <section class="py-16 lg:py-20">
    <div class="container mx-auto px-4 sm:px-6 lg:px-8">
      <div class="max-w-6xl mx-auto">
        <div class="grid lg:grid-cols-2 gap-12 items-center">
          <div>
            <h2 class="text-3xl lg:text-4xl font-serif font-bold text-monica-navy mb-6" data-cms="what-is-title">
              What is a Mastermind?
            </h2>
